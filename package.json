{
  "name": "munisense-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-nl": "ng serve --configuration=nl",
    "build": "ng build --prod",
    "build-nl": "ng build --prod --configuration=nl",
    "test": "ng test",
    "code-coverage": "ng test --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-i18n": "ng xi18n --output-path locale --i18n-locale en  && xliffmerge --profile xliffmerge.json en nl",
    "extract-ngx-translate": "ngx-translate-extract --input ./src --clean --sort --format namespaced-json -o ./src/assets/i18n/nl.json ./src/assets/i18n/en.json  --format-indentation \"  \"",
    "npm install": "npm install"
  },
